<?xml version="1.0" ?>
<template xmlns="http://schemas.sulu.io/template/template"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:xi="http://www.w3.org/2001/XInclude"
          xsi:schemaLocation="http://schemas.sulu.io/template/template http://schemas.sulu.io/template/template-1.0.xsd">

    <key>sponsors-2019</key>

    <meta>
        <title lang="en">Sponsors-2019</title>
        <title lang="de">Sponsors-2019</title>
    </meta>

    <properties>
        <property name="title" type="text_line" mandatory="true">
            <meta>
                <title lang="en">Title</title>
                <title lang="de">Titel</title>
            </meta>
        </property>
        <section name="main_sponsor">
            <meta>
                <title lang="en">Main Sponsor</title>
            </meta>
            <xi:include href="../includes/sponsor.xml"/>
        </section>
        <section name="sponsors">
            <properties>
                <block name="sponsors" default-type="sponsor">
                    <types>
                        <type name="sponsor">
                            <xi:include href="../includes/sponsor.xml"/>
                        </type>
                    </types>
                </block>
            </properties>
        </section>
        <section name="partners">
            <properties>
                <block name="partners" default-type="partner">
                    <types>
                        <type name="partner">
                            <xi:include href="../includes/sponsor.xml"/>
                        </type>
                    </types>
                </block>
            </properties>
        </section>
    </properties>
</template>
